{
  "name": "webpack-5-example",
  "version": "1.0.0",
  "description": "A webpack config that minifies an already working website",
  "main": "index.js",
  "scripts": {
    "start": "reload --browser --dir src",
    "build": "webpack",
    "serve": "http-server public",
    "quality": "prettier --write . && eslint . --fix",
    "quality:check": "prettier -c . && eslint .",
    "deploy:login": "surge login",
    "deploy:logout": "surge logout",
    "deploy": "surge src https://big-yak.surge.sh"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tylergrinn/webpack-5.git"
  },
